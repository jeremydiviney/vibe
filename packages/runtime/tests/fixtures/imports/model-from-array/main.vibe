model testModel = {
  name: "gpt-4",
  provider: "openai",
  apiKey: "test-key",
  url: "https://api.openai.com/v1"
}

const models = [testModel]

function getModel() {
  return models[0]
}

function useModel(m: model) {
  const result: text = do "Say hello" m
  return result
}

const myModel = getModel()
let output = useModel(myModel)
