{
  "name": "vibe",
  "version": "1.0.0",
  "description": "AI agent orchestration language and runtime",
  "main": "src/index.ts",
  "type": "module",
  "bin": {
    "vibe": "src/index.ts"
  },
  "scripts": {
    "start": "bun run src/index.ts",
    "dev": "bun --watch src/index.ts",
    "test": "bun test --exclude '**/*.integration.test.ts'",
    "test:watch": "bun test --watch --exclude '**/*.integration.test.ts'",
    "test:integration": "bun test --include '**/*.integration.test.ts'",
    "test:all": "bun test"
  },
  "keywords": ["ai", "agent", "orchestration", "dsl", "language"],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bun": "latest"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@google/genai": "^1.34.0",
    "chevrotain": "^11.0.3",
    "openai": "^6.15.0"
  }
}
